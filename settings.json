[
    {
        "name": "Uncap FPS",
        "description": "Allows you to play the game at more FPS than your screen refresh rate, which is usually 60 FPS. (Requires client restart)",
        "id": "uncapFrames",
        "category": "Utility",
        "type": "toggle"
    },
    {
        "name": "Resource Swapper",
        "description": "Enables the resource swapper, an addon used to load custom textures, models, css into clients. (Requires refresh)",
        "id": "resourceSwapper",
        "category": "Utility",
        "type": "toggle"
    },
    {
        "name": "Updates",
        "description": "Enables the update checker, which checks for client updates on startup.",
        "id": "update",
        "category": "Utility",
        "type": "toggle"
    },
    {
        "name": "ANGLE Backend",
        "description": "Change the ANGLE backend the client uses. (Requires restart)",
        "id": "angleBackend",
        "category": "Utility",
        "type": "select",
        "options": [
            { "name": "Default", "value": "default" },
            { "name": "Direct3D 11", "value": "d3d11" },
            { "name": "Direct3D 9", "value": "d3d9" },
            { "name": "Direct3D 11 on 12", "value": "d3d11on12" },
            { "name": "OpenGL", "value": "gl" },
            { "name": "Vulkan", "value": "vulkan" },
            { "name": "SwiftShader", "value": "swiftshader" }
        ]
    },
    {
        "name": "Use WebGL 2",
        "description": "Enable WebGL 2 rendering. (Requires restart)",
        "id": "webgl2",
        "category": "Utility",
        "type": "toggle"
    },
    {
        "name": "Enable hardware acceleration",
        "description": "Enable hardware acceleration. (Requires restart)",
        "id": "hardwareAcceleration",
        "category": "Utility",
        "type": "toggle"
    },
    {
        "name": "OBS Game Capture (Windows only)",
        "description": "Allows OBS Studio to capture the game using a 'Game Capture' source. (Requires restart)",
        "id": "gameCapture",
        "category": "Utility",
        "type": "toggle"
    },
    {
        "name": "Client Version",
        "type": "custom",
        "category": "Utility",
        "customHTML": "<span id='clientVersion' style='color: white'></span>"
    },
    {
        "name": "Changelog",
        "type": "button",
        "label": "Open",
        "category": "Utility",
        "onclick": "closWind(); document.getElementById('clientPopupCont').style.display = 'flex'; open_client_changelog()"
    },
    {
        "name": "Show custom badges",
        "description": "Shows RAYS client exclusive badges.",
        "id": "badges",
        "category": "Style",
        "type": "toggle"
    },
    {
        "name": "Show clan tags",
        "description": "Shows RAYS client exclusive clan tags.",
        "id": "clanTags",
        "category": "Style",
        "type": "toggle"
    },
    {
        "name": "Nuke Timer",
        "description": "Shows the time remaining until the nuke explodes next to ammo.",
        "id": "nukeTimer",
        "category": "Style",
        "type": "toggle"
    },
    {
        "name": "Replace \"CLICK TO PLAY\" with timer",
        "description": "Replaces the \"CLICK TO PLAY\" text with the time of game remaining.",
        "id": "clickToPlayReplacer",
        "category": "Style",
        "type": "toggle"
    },
    {
        "name": "Sky Image",
        "description": "Replaces the sky with a custom image.",
        "id": "skyImage",
        "category": "Style",
        "type": "input",
        "placeholder": "Custom Image URL"
    },
    {
        "name": "Loading Image URL",
        "description": "Replaces the background of the loading screen with a custom image.",
        "id": "loadingBackground",
        "category": "Style",
        "type": "input",
        "placeholder": "Custom Image URL"
    },
    {
        "name": "Fast Classes",
        "description": "Shows comp class selector for fast class switching.",
        "id": "fastClasses",
        "category": "Style",
        "type": "toggle"
    },
    {
        "name": "Ad Block (CSS modification)",
        "description": "Removes the banner ads from the game.",
        "id": "adBlock",
        "category": "Style",
        "type": "toggle"
    },
    {
        "name": "Discord Rich Presence",
        "description": "Enables the Discord Rich Presence, which shows what you're doing in the game on Discord.",
        "id": "rpc",
        "category": "Social",
        "type": "select",
        "options": [
            { "name": "All", "value": "all" },
            { "name": "Anonymous", "value": "anon" },
            { "name": "None", "value": "off" }
        ]
    },
    {
        "name": "Twitch Integration",
        "category": "Twitch Integration",
        "label": "Manage",
        "type": "button",
        "onclick": "closWind(); document.getElementById('clientPopupCont').style.display = 'flex'; open_twitch_settings()"
    },
    {
        "name": "Show player ping",
        "description": "Replaces the WiFi icon in the player list with the player's ping.",
        "id": "kpd_ping_addon",
        "category": "KPD",
        "type": "toggle"
    },
