            "min": 0,
            "max": 100,
            "step": 1
        },
        {
            "name": "Scale",
            "type": "slider",
            "id": "spotify.nowPlaying.scale",
            "min": 0.1,
            "max": 2,
            "step": 0.1            
        },
        {
            "name": "Border radius (%)",
            "type": "slider",
