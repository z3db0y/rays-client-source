    "Keystrokes": [
        {
            "name": "Enable",
            "description": "Enables keystrokes.",
            "type": "toggle",
            "id": "keystrokes.enable",
            "refresh": true
        },
        {
            "name": "Offset X (%)",
            "type": "slider",
            "id": "keystrokes.offsetX",
            "min": 0,
            "max": 100,
            "step": 1
        },
        {
            "name": "Offset Y (%)",
            "type": "slider",
            "id": "keystrokes.offsetY",
            "min": 0,
            "max": 100,
            "step": 1
        },
        {
            "name": "Scale",
            "type": "slider",
            "id": "keystrokes.scale",
            "min": 0.1,
            "max": 2,
            "step": 0.1            
        },
        {
            "name": "Border radius (%)",
            "type": "slider",
            "id": "keystrokes.borderRadius",
            "min": 0,
            "max": 50,
            "step": 1
        },
        {
            "name": "Color (normal)",
            "type": "select_with_color",
            "id": "keystrokes.color.normal",
            "options": [
                { "name": "Normal", "value": 0 },
                { "name": "RGB", "value": "rgb" }
            ]
        },
        {
            "name": "Color (pressed)",
            "type": "select_with_color",
            "id": "keystrokes.color.pressed",
            "options": [
                { "name": "Normal", "value": 0 },
                { "name": "RGB", "value": "rgb" }
            ]
        },
        {
            "name": "Background color (normal)",
            "type": "select_with_color",
            "id": "keystrokes.bg.normal",
            "options": [
                { "name": "Normal", "value": 0 },
                { "name": "RGB", "value": "rgb" }
            ]
        },
        {
            "name": "Background color (pressed)",
            "type": "select_with_color",
            "id": "keystrokes.bg.pressed",
            "options": [
                { "name": "Normal", "value": 0 },
                { "name": "RGB", "value": "rgb" }
            ]
        }
    ],
    "Spotify Integration": [
        {
            "name": "Not logged in",
            "type": "custom",
            "customHTML": "<div class='settingsBtn' id='spotifyLogin'>Login</div>"
        },
        {
            "name": "Now playing overlay",
            "type": "toggle",
            "description": "Shows the currently playing song as an overlay.",
