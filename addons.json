    "Twitch Integration": [
        {
            "name": "Enable",
            "description": "Enables Twitch integration.",
            "type": "toggle",
            "id": "twitch.enable"
        },
        {
            "name": "Not logged in",
            "type": "custom",
            "customHTML": "<div class='settingsBtn' id='twitchLogin'>Login</div>"
        },
        {
            "name": "Custom chat commands",
            "type": "button",
            "id": "editTwitchCommands",
