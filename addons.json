{
    "Basic": [
        {
            "name": "Menu timer",
            "description": "Shows the game timer instead of CLICK TO PLAY.",
            "type": "toggle",
            "id": "menuTimer",
            "refresh": true
        },
        {
            "name": "Show player ping",
            "description": "Replaces the ping icon with the player's ping.",
            "type": "toggle",
            "id": "kpdPingAddon",
            "refresh": true
        },
        {
            "name": "Placebo FPS",
            "description": "Multiplies the game's FPS (does not actually increase FPS).",
            "type": "slider",
            "id": "placeboFPS",
            "min": 1,
            "max": 100,
            "step": 0.1,
            "refresh": true
        },
        {
            "name": "Message filters",
            "description": "Filters out chat messages containing the specified words.",
            "type": "button",
            "label": "Edit",
            "onclick": "editMsgFilter()"
        },
        {
            "name": "TradeAssistant",
            "description": "A tool to help you trade.",
            "type": "toggle",
            "id": "tradeAssistant",
            "refresh": true
        }
    ],
    "Discord Rich Presence": [
        {
            "name": "Presence Type",
            "type": "select",
            "id": "rpc.type",
            "options": [
                { "name": "All", "value": "all" },
                { "name": "Anonymous (Allows custom buttons)", "value": "anon" },
                { "name": "None", "value": "off" }
            ]
        },
        {
            "name": "Custom Buttons",
            "type": "button",
            "label": "Edit",
            "onclick": "rpcButtonEditor()"
        }
    ],
